    "def FibonaChicken(n):\n",
    "    if n == 1 :\n",
    "        number = n\n",
    "        \n",
    "    else : \n",
    "        n = n-1 \n",
    "        number = n\n",
    "    \n",
    "    return number\n",
    "\n",
    "\n",
    "\n",
    "n = int(input())\n",
    "FibonaChicken(n)"
